{

    "Default": {
        "rate_limit": 1000, 
        "timeout": 20000, 
        "pre_delay": 200, 
        "post_delay": 200 
    },
    "recognition": {
        "type": "TemplateMatch",
        "param": {
            "threshold": 0.7,
            "method": 5
        }
    },
    "OCR": {
        "recognition": "OCR",
        "threshold": 0.3, 
        "order_by": "Horizontal", 
        "replace": [] 
    },
    "Click": {
        "action": "Click",
        "target": true 
    },
    "LongPress": {
        "action": "LongPress",
        "target": true,
        "duration": 1000 
    },
    "Swipe": {
        "action": "Swipe",
        "duration": 200 
    },
    "focus":{
        "Node.Recognition.Starting": "{name}"
    }
}
